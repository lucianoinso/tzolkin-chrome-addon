{
    "manifest_version": 3,
    "name": "Tzolkin calculator",
    "version": "1.20",
    "description": "Presents a right-click menu item to open a link to the Kin calculation given a selected date.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["contextMenus"],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
    }
}